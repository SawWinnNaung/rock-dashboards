{
  "suricata_index": {
    "order": 0,
    "version": 1,
    "index_patterns": [
      "suricata-*"
    ],
    "settings": {
      "index": {
        "analysis": {
          "analyzer": {
            "on_dots": {
              "filter": "lowercase",
              "type": "custom",
              "tokenizer": "dots_tokenizer"
            }
          },
          "tokenizer": {
            "dots_tokenizer": {
              "pattern": "\\.",
              "type": "pattern"
            }
          }
        }
      }
    },
    "mappings": {
      "_doc": {
	      "dynamic_templates": [
          {
            "ip_addresses": {
              "match_mapping_type": "string",
              "match": "*_ip",
              "mapping": {
                "type": "ip"
              }
            }
          },
          {
            "strings_as_keywords": {
              "match_mapping_type": "string",
              "match": "*",
              "mapping": {
                "type": "keyword"
              }
            }
          }
        ],
        "properties": {
          "@timestamp": {
            "type": "date"
          },
          "@version": {
            "type": "text",
            "fields": {
              "keyword": {
                "type": "keyword",
                "ignore_above": 256
              }
            }
          },
          "flow": {
            "properties": {
              "age": {
                "type": "long"
              },
              "alerted": {
                "type": "boolean"
              },
              "bytes_toclient": {
                "type": "long"
              },
              "bytes_toserver": {
                "type": "long"
              },
              "end": {
                "type": "date"
              },
              "pkts_toclient": {
                "type": "long"
              },
              "pkts_toserver": {
                "type": "long"
              },
              "start": {
                "type": "date"
              }
            }
          },
          "flow_id": {
            "type": "long"
          },
          "http": {
            "properties": {
            "http_user_agent": {
              "type": "text",
              "fields": {
                "keyword": {
                  "type": "keyword",
                  "ignore_above": 2048
                }
              }
            }
}
          },
        "icmp_code": {
          "type": "long"
        },
        "icmp_type": {
          "type": "long"
        },
        "offset": {
          "type": "long"
        },
        "src_port": {
          "type": "long"
        },
        "stats": {
          "properties": {
            "app_layer": {
              "properties": {
                "flow": {
                  "properties": {
                    "dcerpc_tcp": {
                      "type": "long"
                    },
